  "description": "",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/JacobFischer/test"
  },
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/lodash": "4.14.117",
    "@types/node": "10.12.0",
    "@types/webpack": "4.4.17",
    "@types/webpack-env": "1.13.6",
    "babel-loader": "8.0.4",
    "babel-plugin-lodash": "3.3.4",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "lodash": "4.17.11",
    "shx": "0.3.2",
    "ts-loader": "5.2.2",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "typescript": "3.1.3",
    "webpack": "4.21.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:base": "webpack --progress --colors",
    "build:dev": "npm run build:base -- --mode development",
    "build:prod": "npm run build:base -- --mode production",
    "build:stats": "webpack --profile --json > webpack-stats.json",
    "clean": "npm run clean:build && npm run clean:stats",
    "clean:build": "shx rm -rf dist/",
    "clean:stats": "shx rm -rf webpack-stats.json",
    "analyze:stats": "webpack-bundle-analyzer webpack-stats.json",
    "analyze": "npm run build:stats && npm run analyze:stats",
    "server": "http-server dist/",
    "watch": "webpack-dev-server --progress --colors --host 0.0.0.0 --port 8080 --content-base dist/ --mode development",
    "start": "npm run watch",
    "test": "npm run build"
  }
}
